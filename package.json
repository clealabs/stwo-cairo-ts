{
  "name": "stwo-cairo",
  "version": "1.0.5",
  "description": "A Typescript library for stwo-cairo using WASM64",
  "type": "module",
  "main": "./dist/stwo-cairo.umd.cjs",
  "module": "./dist/stwo-cairo.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/stwo-cairo.js",
      "require": "./dist/stwo-cairo.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "build:wasm": "cd backend && cargo build -Z build-std=std,panic_abort --target wasm64-unknown-unknown --release",
    "test:browser": "vitest --config=vitest.browser.config.ts"
  },
  "publishConfig": {
    "provenance": true
  },
  "homepage": "https://github.com/clealabs/stwo-cairo-ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clealabs/stwo-cairo-ts.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@vitest/browser": "^3.2.4",
    "playwright": "^1.55.0",
    "typescript": "~5.8.3",
    "vite": "^7.1.2",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-wasm": "^3.5.0",
    "vitest": "^3.2.4"
  }
}
